{
    "name": "nirunfa/easyapplets",
    "description": "easyapplets for multiple mini programs's custom SDK application",
    "type": "library",
    "require": {
        "php": ">=7.2",
        "ext-fileinfo": "*",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": ">=6.2",
        "ext-simplexml": "*",
        "monolog/monolog": ">=2.0",
        "symfony/http-foundation": "^2.7|^3.0|^4.0|^5.0",
        "psr/simple-cache": "^1.0",
        "phpfastcache/phpfastcache": "^8.0",
        "predis2/predis2": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": ">=7.5.15"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "nirunfa",
            "email": "nirunfa@foxmail.com"
        }
    ],
    "minimum-stability": "beta",
    "autoload": {
        "psr-4": {
            "EasyApplets\\": "src/",
            "EasyApplets\\WechatPrograms\\": "src/WechatPrograms/",
            "EasyApplets\\BaiduPrograms\\": "src/BaiduPrograms/"
        }
    }
}
